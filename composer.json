{
    "name": "vegas-cmf/media",
    "type": "library",
    "description": "Vegas CMF Media Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amsterdam Standard Vegas Team",
            "email": "vegas@amsterdam-standard.pl"
        }
    ],
    "repositories": [
        {
            "type" : "git",
            "url" : "https://github.com/spekkionu/wideimage.git"
        },
        {
            "type" : "git",
            "url" : "https://github.com/arius86/core.git"
        },
        {
            "type" : "git",
            "url" : "https://github.com/arius86/forms.git"
        },
        {
            "type" : "git",
            "url" : "https://github.com/arius86/validation.git"
        },
        {
            "type" : "git",
            "url" : "https://github.com/arius86/filter.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": "2.*",
        "vegas-cmf/core" : "v2.0.x-dev",
        "vegas-cmf/forms" : "v2.0.x-dev",
        "vegas-cmf/validation" : "v2.0.x-dev",
        "vegas-cmf/filter" : "v2.0.x-dev",
        "spekkionu/wideimage" : "dev-master"
    },
    "require-dev": {
        "phalcon/devtools": "1.3.x-dev",
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vegas\\": "src/",
            "Vegas\\Tests\\": "tests/"
        }
    }
}
